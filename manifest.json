{
	"manifest_version": 3,
	"name": "A 1 My Extension",
	"version": "1.0.1",
	"description": "Hello Chrome world!",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"action": {
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		},
		"default_title": "Timer Extension",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"permissions": ["storage"],
	"background": {
		"service_worker": "background.js"
	}
}
